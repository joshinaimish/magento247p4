<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="student" resource="default" engine="innodb" comment="Student Table">
		<column xsi:type="int" name="id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="id"/>
		</constraint>
		<column name="name" nullable="true" xsi:type="varchar" comment="name" length="255"/>
		<column name="city" nullable="true" xsi:type="varchar" comment="city" length="255"/>
		<column name="degree" nullable="true" xsi:type="varchar" comment="Degree" length="255"/>
	</table>
</schema>
