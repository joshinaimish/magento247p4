<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd"> 
    <update handle="customer_account"/> 
     <head>
         <css src="Nxtech_TicketManager::css/ticketmanager.css"/>
    </head>
    <body> 
        <referenceBlock name="page.main.title"> 
            <action method="setPageTitle"> 
                <argument translate="true" name="title" xsi:type="string">Edit Ticket</argument> 
            </action> 
        </referenceBlock> 
        <referenceContainer name="content"> 
            <block class="Nxtech\TicketManager\Block\TicketManager" name="edit-ticket" cacheable="false" template="Nxtech_TicketManager::ticket/edit.phtml" />
        </referenceContainer> 
    </body> 
</page>